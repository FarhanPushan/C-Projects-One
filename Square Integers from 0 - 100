*/




int main(){


	double integer;
	cout << "Please enter an integer: " << endl;
	cin >> integer ;
	
	

	while(integer <= 0 || integer >= 100){
		
		cout << "Please re-enter: " << endl;
		cin >> integer;
		
		
	}
	
	integer = pow(integer, 2);
	cout << "Number squared is " << integer << endl;
	
	
		
	
}
