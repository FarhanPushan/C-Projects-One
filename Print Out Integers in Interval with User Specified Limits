/*

Write a program print-interval.cpp that asks 
the user to input two integers L and U 
(representing the lower and upper limits of the interval),
 and print out all integers in the range L â‰¤ i < U separated by 
spaces. Notice that we include the lower limit, but exclude the upper limit.
*/


#include <iostream>

using namespace std; 

int main(){
	
	int lower;
	int upper;
	
	cout << "Please enter L: " ;
	cin >> lower;

	cout << "Please enter U: " ;
	cin >> upper;

	for(int i = lower; i < upper; i++) {
	
		cout << i << " " ; 


	}

}
