/*

Farhan Pushan




Write a program fibonacci.cpp, 


which uses an array of ints to


compute and print all Fibonacci numbers from F(0) to F(59).

/////////////////////



/////////////////



/////////////////


/////////////////////

*/


#include <iostream>

using namespace std;

int main(){

	    int a = 0 , b = 1, c, i;

    

    cout << a << endl;

    

    cout << b << endl;

    

    for (i = 0; i <=60; i++)

    {

        c = a +b;

        cout << c << endl;

        a = b;

        b = c;

    }

    cout << b  << endl;






}
